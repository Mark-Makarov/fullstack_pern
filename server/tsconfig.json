{
  "compilerOptions": {
    "types": [
      "node"
    ],
    "lib": [
      "ESNext"
    ],
    "target": "ESNext",
    "module": "NodeNext",
    "rootDir": "./",
    "outDir": "./build",
    "esModuleInterop": true,
    "strict": false,
    "sourceMap": true,
    "moduleResolution": "NodeNext",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "isolatedModules": false,
    "noImplicitAny": false,
    "useUnknownInCatchVariables": false,
    "inlineSourceMap": true
  },
    "ts-node": {
      "esm": true
    },
    "exclude": [
      "../node_modules"
    ]
  }